{
    "name": "nestjs-boilertemplate",
    "version": "0.0.1",
    "description": "Nest.js boilertemplate",
    "author": "John Olatubosun <toluolatubosun@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "nest build && npx typeorm -d dist/db/data-source.js migration:run",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "nest build && npx typeorm -d dist/db/data-source.js",
        "migration:generate": "yarn typeorm -- migration:generate",
        "migration:run": "yarn typeorm -- migration:run",
        "migration:revert": "yarn typeorm -- migration:revert"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.658.0",
        "@aws-sdk/client-ses": "^3.637.0",
        "@aws-sdk/lib-storage": "^3.658.0",
        "@aws-sdk/s3-request-presigner": "^3.658.0",
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/cache-manager": "^2.2.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/platform-socket.io": "^10.4.3",
        "@nestjs/swagger": "^7.4.0",
        "@nestjs/typeorm": "^10.0.2",
        "@nestjs/websockets": "^10.4.3",
        "@react-email/components": "^0.0.15",
        "@sentry/nestjs": "^8.30.0",
        "@sentry/profiling-node": "^8.30.0",
        "@socket.io/admin-ui": "^0.5.1",
        "@socket.io/redis-adapter": "^8.3.0",
        "add": "^2.0.6",
        "bcryptjs": "^2.4.3",
        "cache-manager": "^4.0.0",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.5",
        "express-basic-auth": "^1.2.1",
        "lodash": "^4.17.21",
        "ms": "^2.1.3",
        "nodemailer": "^6.9.14",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.12.0",
        "react": "^18.2.0",
        "redis": "^4.7.0",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "socket.io": "^4.8.0",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.20",
        "yarn": "^1.22.22"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/cache-manager": "^4.0.6",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/lodash": "^4.17.7",
        "@types/ms": "^0.7.34",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.3.1",
        "@types/nodemailer": "^6.4.15",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/react": "^18.3.4",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "resolutions": {
        "string-width": "4.2.3",
        "strip-ansi": "6.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
